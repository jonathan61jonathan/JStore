class Sell_Paid extends Invoice {
    private InvoiceType INVOICE_TYPE= "Sell";
    private InvoiceStatus INVOICE_STATUS = "Unpaid";

    public Sell_Installment(int id, Item item, String date, int totalItem, int totalPrice, String dueDate){
      super.id = id;
      super.item = item;
      super.date = date;
      super.totalItem = totalItem;
      super.totalPrice = totalPrice;
    }

    public InvoiceStatus getInvoiceStatus()
    {
        return status;
    }

    public void setInvoiceStatus(InvoiceStatus status)
    {
        super.status = status;
    }

    public void setInstallmentPrice()

    public void printData()
    {
      System.out.println("============INVOICE===========");
      System.out.println("ID:"+id);
      System.out.println("Date:"+date);
      System.out.println("Item:"+Item.getName());
      System.out.println("Total Item:"+ totalItem);
      System.out.println("Total Price:"+ totalPrice);
      System.out.println("Status:"+ status);
    }
}
